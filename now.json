{
  "name": "size-plugin-bot",
  "version": 2,
  "alias": ["size-plugin-bot"],
  "env": {
    "APP_ID": "@size-plugin-bot-app-id",
    "WEBHOOK_SECRET": "@size-plugin-bot-webhook-secret",
    "PRIVATE_KEY":"@size-plugin-bot-private-key-base64-encoded"
  },
  "builds": [{ "src": "index.js", "use": "@now/node" }],
  "routes": [{ "src": "/(.*)", "dest": "/index.js" }]
}
